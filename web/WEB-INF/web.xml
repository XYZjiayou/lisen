<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">


    <servlet>
        <description>loginAction</description>
        <display-name>loginAction</display-name>
        <servlet-name>loginAction</servlet-name>
        <servlet-class>Controller.login.loginAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginAction</servlet-name>
        <url-pattern>Controller/login/loginAction</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>ToJSON</description>
        <display-name>ToJSON</display-name>
        <servlet-name>ToJSON</servlet-name>
        <servlet-class>View.ToJSON</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ToJSON</servlet-name>
        <url-pattern>View/ToJSON</url-pattern>
    </servlet-mapping>



</web-app>
